{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@styles/*": [
        "./src/assets/styles/*"
      ],
      "@images/*": [
        "./src/assets/images/*"
      ],
    }
  }
}
